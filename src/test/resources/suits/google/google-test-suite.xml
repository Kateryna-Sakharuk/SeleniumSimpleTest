<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="GoogleSuite" parallel="false" thread-count="1">
    <parameter name="browserName" value="chrome" />
    <parameter name="testEnv" value="remote"/>
    <parameter name="testData" value="googleTestData.properties"/>
    <listeners>
        <listener class-name="io.qameta.allure.testng.AllureTestNg" />
        <listener class-name="core.report.TestResultsListener" />
        <listener class-name="core.report.TestReporter" />
    </listeners>
    <test name="GoogleSearchProduct">
        <classes>
            <class name="websites.google.GoogleSearchResultTest" />
        </classes>
    </test>
</suite>